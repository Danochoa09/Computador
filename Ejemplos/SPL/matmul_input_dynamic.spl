var A[10][10]
var B[10][10]
var C[10][10]
var f1
var c1
var f2
var c2
var i
var j
var k
var sum

print("Enter f1:")
f1 = input()
print("Enter c1:")
c1 = input()
print("Enter f2:")
f2 = input()
print("Enter c2:")
c2 = input()

# Check dimensions: c1 must equal f2
if c1 == f2 :
    # dimensions ok
end
if c1 != f2 :
    print("Incompatible dimensions")
