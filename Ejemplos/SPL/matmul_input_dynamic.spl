end

# Read matrix A
i = 0
while i < f1 :
    j = 0
    while j < c1 :
        print("A element [", i + 1, "][", j + 1, "]: ")
        A[i][j] = input()
        j = j + 1
    end
    i = i + 1
end

# Read matrix B
i = 0
while i < f2 :
    j = 0
    while j < c2 :
        print("B element [", i + 1, "][", j + 1, "]: ")
        B[i][j] = input()
        j = j + 1
    end
    i = i + 1
end

# Multiply into C
i = 0
while i < f1 :
    j = 0
    while j < c2 :
        sum = 0
        k = 0
        while k < c1 :
            sum = sum + A[i][k] * B[k][j]
            k = k + 1
        end
        C[i][j] = sum
        j = j + 1
    end
    i = i + 1
end

# Print result
i = 0
while i < f1 :
    j = 0
    while j < c2 :
        print(C[i][j])
        j = j + 1
    end
    i = i + 1
end
