# =====================================================
# EJEMPLO 2: Uso de STRUCT (sinónimo de TYPE)
# Define un rectángulo y calcula su área
# =====================================================

#define OUTPUT_ADDR 131072

# Definir struct Rectangle con ancho y alto
struct Rectangle { width, height }

# Crear instancia
var rect : Rectangle
var area

# Inicializar rectángulo
rect.width = 5
rect.height = 8

print("Rectángulo - Ancho: ", rect.width)
print("Rectángulo - Alto: ", rect.height)

# Calcular área
area = rect.width * rect.height

print("Área del rectángulo: ", area)

# Guardar resultado
M[OUTPUT_ADDR] = area
